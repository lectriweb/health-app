            <h3>EMERGENCY REQUEST FORM</h3>
            <p>This form lets us keep your history</p>
            <hr>
              <form action="/ems" method="POST">
                <div class="form-row">
                  <div class="form-group col-md-4">
                    <label for="inputEmail4">First name</label>
                    <input type="text" class="form-control" id="inputEmail4" placeholder="First name" name="fname" required>
                  </div>
                  <div class="form-group col-md-4">
                    <label for="inputPassword4">Last Name</label>
                    <input type="text" class="form-control" id="inputPassword4" placeholder="Last Name" name="lname" required>
                  </div>
                  <div class="form-group col-md-4">
                    <label for="inputPassword4">sex</label>
                      <select class="form-control" id="inputPassword4"  aria-label="Default select example" name="sex">
                      <option selected>none</option>
                      <option>male</option>
                      <option>female</option>
                      </select>
                  </div>
                </div>
                <div class="form-row">
                  <div class="form-group col-md-4">
                    <label for="inputEmail4">Height</label>
                    <input type="text" class="form-control" id="inputEmail4" placeholder="HEIGHT" name="height" required>
                  </div>
                <div class="form-row">
                  <div class="form-group col-md-4">
                    <label for="inputEmail4">family sickness</label>
                    <input type="text" class="form-control" id="inputEmail4" placeholder="FAMILY SICKNESS" name="familysickness" required>
                  </div>
                  <div class="form-group col-md-4">
                    <label for="inputEmail4">Blood Type</label>
                    <input type="text" class="form-control" id="inputEmail4" placeholder="BLOOD TYPE" name="bloodtype" required>
                  </div>
                  <div class="form-row">
                    <div class="form-group col-md-4">
                      <label for="inputEmail4">Blood Group</label>
                      <input type="text" class="form-control" id="inputEmail4" placeholder="BLOOD GROUP" name="bloodgroup" required>
                    </div>
                  <div class="form-group col-md-4">
                    <label for="inputPassword4">Alternate Number</label>
                    <input type="text" class="form-control" id="inputPassword4" placeholder="KIN NUMBER" name="anumber" required>
                  </div>
                </div>
                <div class="form-row">
                  <div class="form-group col-md-6">
                    <label for="inputEmail4">Current Address</label>
                    <input type="text" class="form-control" id="inputEmail4" placeholder="CONTACT ADDRESS" name="caddress" required>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="inputEmail4">Other Emergency Number</label>
                    <input type="text" class="form-control" id="inputEmail4" placeholder="OTHER NUMBER" name="otherno" required>
                  </div>
                </div>
                <div class="form-row">
                  <div class="form-group col-md-6">
                    <label for="inputEmail4">Emergency Type</label>
                    <select class="form-select form-control" aria-label="Default select example" name="situation">
                      <option selected>which type of situation</option>
                      <option>Accident</option>
                      <option>Natural causes</option>
                      <option>other</option>
                    </select>
                  </div>
                </div>
                  <div class="form-row">
                  <div class="form-group col-md-6">
                  </div>
                  <div class="form-group col-md-6">
                    <input type="submit" class="btn btn-lg btn-primary">
                  </div>
                </div>
              </form>
